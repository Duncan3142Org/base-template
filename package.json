{
	"name": "@duncan3142org/base-template",
	"description": "Base repo template",
	"author": "Duncan3142Org",
	"version": "1.4.10",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Duncan3142Org/base-template.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"type": "module",
	"license": "AGPL-3.0-only",
	"engines": {
		"node": ">=24",
		"npm": ">=11"
	},
	"files": [
		".tsc/dist/**/*.js",
		".tsc/dist/**/*.ts",
		".tsc/dist/**/*.map"
	],
	"imports": {
		"#duncan3142org/base-template": "./.tsc/dist/index.js"
	},
	"exports": {
		".": {
			"types": "./.tsc/dist/index.d.ts",
			"import": "./.tsc/dist/index.js"
		}
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^20.4.2",
		"@duncan3142/prettier-config": "^0.1.10",
		"@duncan3142/tsc-config": "^0.1.13",
		"@types/node": "^24.10.13",
		"@duncan3142/eslint-config": "^0.1.20",
		"@eslint/config-inspector": "^1.3.0",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
