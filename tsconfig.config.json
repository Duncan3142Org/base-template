{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["./tsconfig.base.json"],
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"skipLibCheck": true,
		"isolatedDeclarations": false,
		"outDir": "${configDir}/.tsc",
		"tsBuildInfoFile": "${configDir}/.tsc/tsconfig.config.tsbuildinfo"
	},
	"include": ["${configDir}/.*", "${configDir}/*", "${configDir}/eslint/**/*"]
}
